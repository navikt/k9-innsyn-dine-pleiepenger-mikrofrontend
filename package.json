    "@navikt/ds-tokens": "^5.3.1",
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "2.0.3",
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "0.4.0",
