    "@navikt/ds-tokens": "^7.9.1",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.1",
    "postcss-import": "^16.1.0",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.0",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
